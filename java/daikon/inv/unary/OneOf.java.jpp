      } else if (is_hashcode()) {
        String null_str = elts[0] == 0 ? " == null" : " != null"; // interned
        result = "Contract.ForAll(" + split[0] + ", x => x" + split[1] + null_str + ")";
      } else
    #endif
      {
        result = "Contract.ForAll(" + split[0] + ", x => x" + split[1] + equalsString + args + endString + ")";
      }
