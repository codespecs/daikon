ac_subdirs_all="$ac_subdirs_all libraries/zlib"
ac_subdirs_all="$ac_subdirs_all libraries/png"
ac_subdirs_all="$ac_subdirs_all libraries/jpeg"
ac_subdirs_all="$ac_subdirs_all libraries/tiff"
ac_subst_vars='SHELL PATH_SEPARATOR PACKAGE_NAME PACKAGE_TARNAME PACKAGE_VERSION PACKAGE_STRING PACKAGE_BUGREPORT exec_prefix prefix program_transform_name bindir sbindir libexecdir datadir sysconfdir sharedstatedir localstatedir libdir includedir oldincludedir infodir mandir build_alias host_alias target_alias DEFS ECHO_C ECHO_N ECHO_T LIBS VERSION_BASE COMPILED_BY C99_COMPATIBLE_RADIOSITY build build_cpu build_vendor build_os host host_cpu host_vendor host_os INSTALL_PROGRAM INSTALL_SCRIPT INSTALL_DATA CYGPATH_W PACKAGE VERSION ACLOCAL AUTOCONF AUTOMAKE AUTOHEADER MAKEINFO AMTAR install_sh STRIP ac_ct_STRIP INSTALL_STRIP_PROGRAM AWK SET_MAKE am__leading_dot MAINTAINER_MODE_TRUE MAINTAINER_MODE_FALSE MAINT C_INCLUDE_PATH CPLUS_INCLUDE_PATH EGREP CXX CXXFLAGS LDFLAGS CPPFLAGS ac_ct_CXX EXEEXT OBJEXT DEPDIR am__include am__quote AMDEP_TRUE AMDEP_FALSE AMDEPBACKSLASH CXXDEPMODE am__fastdepCXX_TRUE am__fastdepCXX_FALSE CXXCPP CC CFLAGS ac_ct_CC CCDEPMODE am__fastdepCC_TRUE am__fastdepCC_FALSE CPP RANLIB ac_ct_RANLIB LIBOBJS X_CFLAGS X_PRE_LIBS X_LIBS X_EXTRA_LIBS subdirs INCZ LIBZ INCPNG LIBPNG INCJPEG LIBJPEG INCTIFF LIBTIFF POVLIBS POVLIBSCLEAN LTLIBOBJS'
ac_subst_files=''

