	@echo
	@${GZCAT} ${DTRACE_FILES} \
		| perl -ne '\
		BEGIN { $$/ = "\n\n";} \
		$$_ =~ /^([^\n]+)\n/; \
		$$x{$$1} += length($$_); \
		END { for (keys %x) { print $$x{$$_}, " ", $$_, "\n"; } }\
		' | sort -n

